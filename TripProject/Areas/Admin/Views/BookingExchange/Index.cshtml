
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model IEnumerable<BookingExchangeModel.Exchange_Rates>
<h1 style="margin-top:15px">TÜRK LİRASI KARŞILIKLARI</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th>PARA BİRİMİ</th>
            <th>DEĞERİ</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.currency</th>
                <th>@item.exchange_rate_buy</th>
            </tr>
        }
    </tbody>
</table>

